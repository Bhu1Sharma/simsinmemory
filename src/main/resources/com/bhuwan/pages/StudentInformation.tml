<html t:type="layout"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
<div class="container">
    <table class="responsive-table">
        <thead>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>Standard</th>
        <th></th>
        </thead>
        <tbody>
        <tr>
            <td>${student.studentId}</td>
            <td>${student.name}</td>
            <td>${student.age}</td>
            <td>${student.standard}</td>
        </tr>
        </tbody>
    </table>
</div>
</html>
